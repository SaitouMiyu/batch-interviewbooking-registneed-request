package jp.rhp.model.connection.recop;

import jp.rhp.model.connection.company.RecopDatabase;

public interface RecopDatabaseConnectionRepository
{
	// recopDBの接続情報を取得
    public RecopDatabase getRecopDatabaseContents();

    // 取得情報を基にrecopDBに接続
    public void connectRecopDatabase( RecopDatabase recopDatabase ) throws Exception;
}
